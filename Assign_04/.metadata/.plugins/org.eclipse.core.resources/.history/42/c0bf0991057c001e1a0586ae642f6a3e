package com.sunbeam;

public class Main {

	public static void main(String[] args) {
		List l1=new List();
		l1.addFirst(10);
		l1.display();
		System.out.println("-----");
		l1.addLast(20);
		l1.display();
		System.out.println("-----");
		l1.addLast(40);
		l1.display();
		System.out.println("-----");
		l1.addPosition(40, 3);
		l1.display();
		System.out.println("-----");
	}

}
