
/*Implement singly circular linked list using tail pointer (no head pointer). 
Compare time complexities of all operations.*/

package com.sunbeam;

public class List 
{
 public class node
 {
	 private int data;
	 private node next;
	 public node(int value)
	 {
		 data=value;
		 next=null;
	 }
 }
	 private node tail;
	 public List()
	 {
		 tail=null;
	 }
 }

