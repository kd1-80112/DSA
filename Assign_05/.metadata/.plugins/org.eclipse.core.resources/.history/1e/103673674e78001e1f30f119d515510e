package com.sunbeam;

public class Main {

	public static void main(String[] args) {
		List l1=new List();
		l1.addLast(10);
		l1.addLast(20);
		l1.addLast(30);
		System.out.println("add last");
		l1.displayAll();
		System.out.println(" count="+l1.count);
		System.out.println("");
		System.out.println("add first");
		l1.addFirst(40);
		l1.displayAll();
		System.out.println(" count="+l1.count);
		System.out.println("");
		System.out.println("del last");
		l1.delLast();
		

	}

}
