package com.sunbeam;

public class List 
{
  public class node
  {
	  private int data;
	  private node next;
	  
	  public node(int value)
	  {
		  data=value;
		  next=null;
	  }
  }
  private node head;
  public int count;
  
  public List()
  {
	  head=null;
	  count=0;
  }
  
  public boolean  isEmpty()
  {
	  return head==null;
  }
  
  public void addLast(int value)
  {
	  node newnode=new node(value);
	  
			  
  }
}
